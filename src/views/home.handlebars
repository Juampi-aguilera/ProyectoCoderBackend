<div>
    <h1>Nuestros productos</h1>
    <ul>
        {{#each docs}}
        <li>{{this.title}}</li>
        {{/each}}
    </ul>
    <div>
        {{#if hasPrevPage}}
            <a href={{this.prevLink}}> << Anterior</a>
        {{/if}}
        &nbsp;
        {{this.page}}
        &nbsp;
        {{#if hasNextPage}}
            <a href={{this.nextLink}}>Siguiente >></a>
        {{/if}} 
    </div>
</div>